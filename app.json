{
  "name": "flareUp",
  "description": "Self-hosted and Easy-to-deploy Cloudflare based Dynamic DNS service for router",
  "repository": "https://github.com/CoolFool/flareup/",
  "stack": "container",
  "env":{
    "USERNAME":{
      "description" : "Username for flareup"
    },
    "PASSWORD":{
      "description" : "Password for flareup"
    },
    "CF_API_TOKEN":{
      "description" : "Cloudflare API token"
    },
    "DOMAINS":{
      "description" : "Comma(,) seperated domains"
    },
    "PROXIED":{
      "description" : "Proxy connection through cloudflare dns?",
      "value": "false"
    }
  }
}
